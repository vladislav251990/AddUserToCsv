@model IEnumerable<UserAdding.Models.User>


<tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
</tr>
@foreach (var user in Model)
{
    <tr>
        <td>@user.FirstName</td>
        <td>@user.LastName</td>
        <td>@user.Address</td>
        <td>@user.City</td>
        <td>@user.State</td>
        <td>@user.Zip</td>
    </tr>
}
